---
const { id, r } = Astro.props;
const src = r ? `/cylindrical_shells_v2.html?r=${r}` : "/cylindrical_shells_v2.html";
---
<div class="step-viz-col">
    <iframe src={src} class="viz-frame" id={id} scrolling="no" style="width: 100%; height: 400px; border: none; border-radius: 8px;"></iframe>
</div>
